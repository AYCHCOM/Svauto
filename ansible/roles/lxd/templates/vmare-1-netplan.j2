# cloud-config
version: 2
ethernets:
    eth0:
        mtu: 1500
        addresses: []
vlans:
    eth0.100:
        mtu: 1500
        id: 100
        link: eth0
        addresses:
        - {{ container.ip }}
        gateway4: {{ container.gw }}
        nameservers:
            addresses: [ {{ dns_servers }} ]
            search: [ {{ dns_servers }} ]
